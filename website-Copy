<!-- THE ESSENTIAL GRID 2.3.2 CUSTOM -->

<article class="myportfolio-container minimal-light source_type_custom esg-revealed" id="esg-grid-8-1-wrap" style="position: relative; z-index: 0; min-height: 100px; height: auto;">

    <div class="esg-container-fullscreen-forcer" style="position: relative; left: 0px; top: 0px; width: auto; height: auto;"><div id="esg-grid-8-1" class="esg-grid esg-layout-masonry esg-container" style="background: transparent;padding: 0px 0px 0px 0px ; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;">
<div class="esg-overflowtrick" style="width: 100%; height: 86px;"><ul class="mainul" style="height: 86px;">
